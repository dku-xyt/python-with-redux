{
  "name": "python-population",
  "version": "0.0.1",
  "author": "@dtodo1paco",
  "license": "MIT",
  "description": "A python sample application using react for UI",
  "main": "server/server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dtodo1paco/python-and-react.git"
  },
  "scripts": {
    "start": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --port 3000 --history-api-fallback",
    "build": "./node_modules/webpack/bin/webpack.js --config webpack.config.js",
    "compress": "./node_modules/webpack/bin/webpack.js --config webpack.config.js",
    "production": "npm run build && npm run server-prod",
    "server-prod": "cd server && export BACKEND_ENV=prod && make prod",
    "server": "cd server && make run",
    "lint-js": "./node_modules/eslint/bin/eslint.js -c './src/config/.eslintrc.js' src/",
    "jest": "./node_modules/jest/bin/jest.js",
    "test": "npm run build && npm run jest && cd server && make tests"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "connected-react-router": "^4.2.3",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.22.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-md": "^1.5.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "redux-freeze": "^0.1.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "clean-webpack-plugin": "^0.1.17",
    "compression-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.16.0",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.1.4",
    "json-loader": "^0.5.4",
    "jsx-loader": "^0.13.2",
    "node-sass": "^4.9.3",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "prop-types": "^15.5.10",
    "react-test-renderer": "^15.2.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.0"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  }
}
